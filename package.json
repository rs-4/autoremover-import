{
  "name": "autoremover-import",
  "version": "1.2.0",
  "description": "remove and auto import package json for javascript",
  "main": "dist/core/importManager.js",
  "types": "dist/core/importManager.d.ts",
  "bin": {
    "autoremover-import": "./src/bin/cli.js"
  },
  "scripts": {
    "dev": "node src/index.js",
    "watch-imports": "node src/bin/cli.js",
    "postinstall": "node src/bin/install.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "echo \"No tests specified\" && exit 0",
    "commit": "git-cz"
  },
  "keywords": [
    "import",
    "dependencies",
    "auto-import",
    "package-manager",
    "javascript",
    "react"
  ],
  "author": {
    "name": "rs12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rs-4/autoremover-import"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "dependencies": {
    "axios": "^1.7.9"
  }
}
